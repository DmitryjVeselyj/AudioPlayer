<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.Music_Player1" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/black</item>
        <item name="colorPrimaryVariant">@color/purple_200</item>
        <item name="colorOnPrimary">@color/purple_200</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/purple_200</item>
        <item name="colorSecondaryVariant">@color/purple_200</item>
        <item name="colorOnSecondary">@color/purple_200</item>
        <item name="colorControlActivated">@color/purple_200</item>
        <!-- Status bar color. -->
        <item name="fontFamily">@font/circular_family</item>
        <item name="android:forceDarkAllowed">false</item>
        <item name="android:statusBarColor" tools:targetApi="l">@color/black</item>
        <item name="android:navigationBarColor" tools:targetApi="l">@color/black</item>
        <item name="android:windowContentTransitions">true</item>
        <item name="android:windowBackground">@color/black</item>
        <item name="android:colorBackground">@color/black</item>


        <!-- Customize your theme here. -->
    </style>
    <style name="MineCustomTabText" parent="TextAppearance.Design.Tab">
        <item name="android:fontFamily">@font/circular_family</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
    </style>

</resources>